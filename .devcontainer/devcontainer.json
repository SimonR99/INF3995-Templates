{
  "name": "humble desktop",
  "dockerComposeFile": [
      "../docker-compose.yml"
  ],
  "service": "ros",
  "workspaceFolder": "/root/workspaces",
  "mounts": [
    "source=${localEnv:HOME}${localEnv:USERPROFILE}/.bash_history,target=/root/.bash_history,type=bind"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-iot.vscode-ros",
        "ms-python.autopep8",
        "ms-python.flake8"
      ]
    }
  }
}